[repositories]
# Define the root cell and prelude
root = .
prelude = buck2-prelude

[buildfile]
name = BUCK
package_boundary = BUCK

[project]
project_ignore = .git, .gitignore, .idea, **/.buckd, **/buck-out, **/buck-out/**

[build]
threads = 0

[parser]
allow_implicit_package = true

[ui]
# Enable the new parser
parser = "v2"